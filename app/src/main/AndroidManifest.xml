<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bensin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/standmeter"
        android:label="@string/app_name"
        android:roundIcon="@drawable/standmeter"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".transaksi_tabel"></activity>
        <activity android:name=".posting_manual" />
        <activity
            android:name=".full_bulanan"
            android:screenOrientation="landscape" />
        <activity
            android:name=".full_harian"
            android:screenOrientation="landscape" />
        <activity android:name=".bulanan" />
        <activity android:name=".Harian" />
        <activity android:name=".transaksi" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>